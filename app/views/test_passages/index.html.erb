<p id="notice"><%= notice %></p>

<h1>Testpassages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testpassages.each do |testpassage| %>
      <tr>
        <td><%= link_to 'Show', testpassage %></td>
        <td><%= link_to 'Edit', edit_testpassage_path(testpassage) %></td>
        <td><%= link_to 'Destroy', testpassage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New TestPassage', new_testpassage_path %>
